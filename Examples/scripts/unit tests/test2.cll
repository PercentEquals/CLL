// Last tested version: 1.1.1

function check
{
	if argv[1] === "NULL"; return "OK\n"
	if argv[0] === argv[1]; return "OK\n"
	return "ERROR (" + argv[0] + " =/= " + argv[1] + ")\n"
}

PI = 3.1415926535897932384

cout "sleep:     " check(sleep(10), 10)
cout "time:      " check(time("s"), time("s"))
cout "rand:      " check(rand(0,10), "NULL")
cout "typeof:    " check(typeof(10.0f), "FLOAT")
cout "int:       " check(int(10.6f), 10)
cout "bool:      " check(bool("10.0f"), 1)
cout "float:     " check(float(10), 10.0f)
cout "double:    " check(double(10), 10.0)
cout "char:      " check(char(97), 'a')
cout "string:    " check(string(10.0f), "10.000000")
cout "fwrite:    " check(fwrite("delete_me.txt", "test_string"), 1)
cout "fappend:   " check(fappend("delete_me.txt", "string_test10"), 1)
cout "fopen:     " check(fopen("delete_me.txt"), ["test_string", "string_test10"])
cout "fexist:    " check(fexist("delete_me.txt"), 1)
cout "find:      " check(find("test_string", 's'), 2)
cout "rfind:     " check(rfind("test_string", 's'), 5)
cout "substr:    " check(substr("test_string", 5), "string")
cout "substr:    " check(substr("test_string", 5), "string")
cout "strspn:    " check(strspn("test_string", "test"), 4)
cout "stoi:      " check(stoi("10.6000"), 10)
cout "stof:      " check(stof("10.6000"), 10.6f)
cout "stod:      " check(stod("10.6000"), 10.6)
cout "to_string: " check(to_string(10.6f), "10.600000")
cout "length:    " check(length([10, 20], [20, 10]), [2, 2])
cout "sqrt:      " check(sqrt(16), 4.0)
cout "cbrt:      " check(cbrt(27), 3.0)
cout "abs:       " check(abs(-10.0f), 10.0f)
cout "hypot:     " check(hypot(3, 4), 5.0)
cout "floor:     " check(floor(10.5), 10.0)
cout "ceil:      " check(ceil(10.5), 11.0)
cout "round:     " check(round(10.5), 11.0)
cout "trunc:     " check(trunc(10.5), 10.0)
cout "cos:       " check(cos(PI), -1.0)
cout "sin:       " check(sin(PI), 0.0)
cout "tan:       " check(tan(PI), 0.0)
cout "acos:      " check(acos(1), 0.0)
cout "asin:      " check(asin(0), 0.0)
cout "atan:      " check(atan(0), 0.0)
cout "cosh:      " check(cosh(0), 1.0)
cout "sinh:      " check(sinh(0), 0.0)
cout "tanh:      " check(tanh(0), 0.0)
cout "acosh:     " check(acosh(1), 0.0)
cout "asinh:     " check(asinh(0), 0.0)
cout "atanh:     " check(atanh(0), 0.0)
cout "exp:       " check(exp(1), 2.718282)
cout "exp2:      " check(exp2(1), 2.0)
cout "ldexp:     " check(ldexp(0.95, 4), 15.2)
cout "log:       " check(log(5.5), 1.704748)
cout "log10:     " check(log10(1000), 3.0)