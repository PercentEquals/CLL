function opposite
{
	ret = ""

	if typeof(params[0]) == "STRING"
	{
		for i = length(params[0]) - 1, i >= 0, i -= 1
		{
			ret += params[0][i]
		}
	}
	else if typeof(params[0]) == "CHAR"; ret = char(params[0] ^ 32)
	else; ret = int((params[0] > 0) ? (-abs(params[0])) : abs(params[0]))
	return ret
}

