cout "How many primes to find: "
cin primes

start = time("ms")

function isPrime
{
	if params[0] <= 2; return (params[0] == 2) ? 1 : 0
	if params[0] % params[1] == 0; return 0
	if params[1] * params[1] > params[0]; return 1

	return isPrime(params[0], params[1] + 1)
}

vec = []
i = 2
while i < primes
{
	if isPrime(i, 2)
		vec += i

	i+=1
}

cout vec endl
cout "TIME: " time("ms")-start
